$j('#tot_votes').html("<%= escape_javascript(render :partial => "total_votes") %>");
<% unless (@vote_topic.total_votes == 0) %>
  if ($j('#vote_dlg').children().size() == 0) {
  $j('#vote_dlg').html("<%= escape_javascript(render :partial => "vote_tally") %>");
  $j("#chart_controls a:contains('Pie')").button({icons:{primary:'ui-icon-document'}, text:false});
  $j("#chart_controls a:contains('Age')").button({icons:{primary:'ui-icon-person'}, text:false});
  $j("#chart_controls a:contains('Gender')").button({icons:{primary:'ui-icon-close'}, text:false});
  }
 <%if current_user && !@vote_items.nil? && !@selected_response.nil? %>
    $j('div_vote_form').html("<%= escape_javascript(render :partial => 'vote_form', :locals => {:submit_url => process_votes_vote_topic_path(@vote_topic, :user_id => current_user.id), :action => 'process'}) %>");
  <% elsif current_user && @selected_response %>
    $j('div_vote_form').html("<%= escape_javascript(render :partial => 'vote_form', :locals => {:submit_url => cancel_vote_vote_topic_path(@vote_topic, :user_id => current_user.id), :action => 'cancel'}) %>");
  <% elsif !current_user %>
    $j('div_vote_form').html("<%= escape_javascript(render :partial => "vote_form_need_login", :locals => {:action => 'login'}) %>");
  <% end  %>
<% else %>
  $j('#vote_dlg').empty();
<% end  %>