<% if selected_response
  display = "none"
else
  display = "block"
end%>

<div class="" id="unvoted_state" style="display:<%= display %>">
  <ul class="top-mar vi-list" id="vote_items_list_unvoted">
    <%@vote_topic.vote_items.each do |v| %>
      <li id="v-item-<%= v.id %>" class="non-sel option-list">
        <a href="#" id="<%= v.id %>" class="vote take-action">
          <span class="ui-icon-check ui-icon vote-icon" ></span>
          <%= v.option.titleize %>
        </a>
      <% end  %>
    </li>
    <li class=" no-style-no-border "><span class="go-right small-pad ui-widget-content">Vote to see results</span></li>
  </ul>
</div>
