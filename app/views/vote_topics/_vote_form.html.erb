  <ul class="top-mar vi-list-voted" id="vote_items_list">
    <% @vote_topic.vote_items.each do |v| %>
      <% if !@selected_response.nil?  && @selected_response.to_i == v.id
        selected = true
        icon = 'ui-icon-close'
      else
        icon = 'ui-icon-check'
      end  %>
      <li id="v-item-<%= v.id %>" class=" <%= selected ? 'sel' : 'non-sel' %> ">
        <% if !@selected_response.nil? && !selected %>
          <%= (get_vote_percent v, @vote_topic.votes_count) %>
        <% else %>
          <a href="#" id="<%= v.id %>" class="<%= selected ? 'cancel' : 'vote'%> take-action">
            <span class="ui-icon <%= icon %> vote-icon" ></span>
            <%= (get_vote_percent v, @vote_topic.votes_count) %></a>
          <% end  %>
      </li>
      <%= get_percent_div (v.get_vote_percent_num @vote_topic.votes_count) %>
      <li class="option-separator"></li>
    <% end %>
  </ul>
