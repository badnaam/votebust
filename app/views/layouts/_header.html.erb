<div class="main-header" id="">
  <div class="header" >
    <div class ="span-24 header_links_div last top-mar">
      <div class="span-10">
        <div class="span-10">
          <%= link_to "", root_path, :class => "go-left logo-image sprite"%>
        </div>
        <div class="span-10">
          <span class="logo_msg small go-left"><%= t 'site_logo_msg' %></span>
          &nbsp;<a href="intro-prezo-trig" class="go-left small special-text bld left-mar" id="" rel="#intro_prezo">
            <%= t ('site_learn_more') %>
          </a>
        </div>
      </div>
      <%= render :partial => "/shared/cat_nav_list" %>
      <%= render :partial => "/shared/city_nav_list" %>
      <ul class="header_links">
        <% unless  current_user %>
          <li><%= link_to "sign up", sign_up_path, :id => 'main_sign_up_link', :class=> 'special-text'%></li>
          <li><%= link_to "login", login_path, :id => 'main_login_link' %></li>
        <% else %>
          <li><%= link_to current_user.username + " profile", current_user, :id => 'main_profile_link' %>
          </li>
          <li><%= link_to "logout", logout_path  %></li>
        <% end %>
        <li>
          <a href="/vote_topics/rss" class="sprite rss-image" id="">
            &nbsp;
          </a>
          <%= auto_discovery_link_tag(:rss, rss_vote_topics_url(:format => 'rss')) %>
        </li>
      </ul>
    </div>

    <!-- <sup>&trade;</sup> -->
    <div class="span-24 last  top-mar">
      <div class="main_links_div span-12">
        <%= render_navigation :level => 1 %>
      </div>
      <% if current_user %>
        <div class="span-6" id="">
          <%= new_button controller %>
        </div>
      <% end  %>
      <div class="div-search-form"
      <% form_tag searches_path, :method => :get, :class => '', :id => "search_form" do %>
        <%= text_field_tag :q, cookies[:voteable_q].nil? ? "Search" : cookies[:voteable_q],  :class => 'search-term' %>
             <a href="#" id="search-button" class="search-button">&nbsp;</a>
        <% end  %>
      </div>
    </div>
  </div>
</div>