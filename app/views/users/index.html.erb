<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
    <th>Username</th>
    <th>Sex</th>
    <th>Age</th>
    <th>Lat</th>
    <th>Lng</th>
    <th>Password</th>
    <th>Role</th>
    <th>Active</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%=h user.email %></td>
    <td><%=h user.username %></td>
    <td><%=h user.sex %></td>
    <td><%=h user.age %></td>
    <td><%=h user.lat %></td>
    <td><%=h user.lng %></td>
    <td><%=h user.password %></td>
    <td><%=h user.role_id %></td>
    <td><%=h user.active %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
    <td><%= link_to 'Destroy', user, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New user', new_user_path %>